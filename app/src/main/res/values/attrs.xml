<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="icvp_min_page_scale_offset" format="dimension"/>
    <attr name="icvp_center_page_scale_offset" format="dimension"/>
    <attr name="icvp_min_page_scale" format="float"/>
    <attr name="icvp_max_page_scale" format="float"/>
    <attr name="icvp_medium_scaled" format="boolean"/>
    <attr name="icvp_scroll_duration" format="integer"/>
    <attr name="icvp_interpolator" format="reference"/>

    <declare-styleable name="HorizontalInfiniteCycleViewPager">
        <attr name="icvp_min_page_scale_offset"/>
        <attr name="icvp_center_page_scale_offset"/>
        <attr name="icvp_min_page_scale"/>
        <attr name="icvp_max_page_scale"/>
        <attr name="icvp_medium_scaled"/>
        <attr name="icvp_scroll_duration"/>
        <attr name="icvp_interpolator"/>
    </declare-styleable>

    <declare-styleable name="VerticalInfiniteCycleViewPager">
        <attr name="icvp_min_page_scale_offset"/>
        <attr name="icvp_center_page_scale_offset"/>
        <attr name="icvp_min_page_scale"/>
        <attr name="icvp_max_page_scale"/>
        <attr name="icvp_medium_scaled"/>
        <attr name="icvp_scroll_duration"/>
        <attr name="icvp_interpolator"/>
    </declare-styleable>

    <declare-styleable name="DashLineView">
        <attr name="lineColor" format="integer" />
        <attr name="patheffect" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SubsamplingScaleImageView">
        <attr name="src" format="reference"/>
        <attr name="assetName" format="string"/>
        <attr name="panEnabled" format="boolean"/>
        <attr name="zoomEnabled" format="boolean"/>
        <attr name="quickScaleEnabled" format="boolean"/>
        <attr name="tileBackgroundColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ColoredRoundView">
        <attr name="crv_selected_color" format="color|reference"/>
        <attr name="crv_border_color" format="color|reference"/>
        <attr name="crv_unselected_color" format="color|reference"/>
        <attr name="crv_border_size" format="dimension|reference"/>
        <attr name="crv_radius" format="dimension|reference"/>
        <attr name="crv_selected" format="boolean" />
    </declare-styleable>

    <declare-styleable name="StepByStepView">
        <attr name="sbsv_selected_color" format="color|reference"/>
        <attr name="sbsv_border_color" format="color|reference"/>
        <attr name="sbsv_unselected_color" format="color|reference"/>
        <attr name="sbsv_border_size" format="dimension|reference"/>
        <attr name="sbsv_radius" format="dimension|reference"/>
        <attr name="sbsv_gap" format="dimension|reference"/>
        <attr name="sbsv_textsize" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="CustomBehavior">
        <attr name="finalHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="bubble">
        <attr name="shadowColor" format="color" />
        <attr name="padding" format="dimension" />
        <attr name="strokeWidth" format="float" />
        <attr name="cornerRadius" format="float" />
        <attr name="halfBaseOfLeg" format="dimension" />
    </declare-styleable>
</resources>